    "def solution(n):\n",
    "    answer = 0\n",
    "    i = 1\n",
    "    while i < n:\n",
    "        if n%i == 1:\n",
    "            answer = i\n",
    "            break\n",
    "        i+=1\n",
    "    \n",
    "    return answer"
   ]
  },
